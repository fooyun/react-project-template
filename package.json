{
  "name": "react-project-template",
  "version": "1.0.0",
  "description": "A react project template, with webpack + typescript + jest + api mock + less + prettier etc.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config=webpack.config.dev.babel.js",
    "start:api": "DATA_MODE=api webpack-dev-server --config=webpack.config.dev.babel.js",
    "build": "webpack --config=webpack.config.prod.babel.js",
    "test": "jest",
    "check": "npm run prettier && npm run test",
    "prettier": "prettier --write './**/*.{js,jsx,ts,tsx,json,css,less}'"
  },
  "author": "fooyun",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/register": "^7.23.7",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "babel-loader": "~8.3.0",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "mockjs": "^1.1.0",
    "prettier": "3.3.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
